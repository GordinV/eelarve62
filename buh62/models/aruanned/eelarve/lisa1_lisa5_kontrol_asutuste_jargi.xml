                                         WHEN $3::INTEGER = 1
                                             THEN rekv_id
                                         ELSE $2::INTEGER END)
                    ORDER BY CASE WHEN r.id = 63 THEN 0 WHEN parentid = 63 THEN 1 ELSE 2 END,
                             r.id::TEXT || '-' || r.parentid ::TEXT, k.idx,
                             CASE WHEN k.nimetus = 'PÃ•HITEGEVUSE TULUD KOKKU' THEN '1' ELSE k.nimetus END</sql>
