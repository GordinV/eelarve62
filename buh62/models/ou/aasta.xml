                          eelarve_kinni,
                          default_,
                          ou.is_last_quarter_opened(aasta.rekvid,
                                                       make_date(aasta.aasta, aasta.kuu, 01))::INTEGER AS kas_veerand_avatud
                   FROM ou.aasta aasta
                   WHERE Aasta.rekvid = $1
